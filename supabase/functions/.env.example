# Supabase Edge Functions Environment Variables
# Copy this file to .env for local development
# For production, set these using: supabase secrets set KEY=VALUE

# OpenAI API Key for summarization, tagging, and embedding
OPENAI_API_KEY=your_openai_api_key_here

# Pinecone API Key and Index Name for vector storage
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_INDEX_NAME=your_pinecone_index_name_here

# These are automatically provided by Supabase:
# SUPABASE_URL
# SUPABASE_ANON_KEY
# SUPABASE_SERVICE_ROLE_KEY
# SUPABASE_DB_URL